{
    "builds": 
    [
        {
            "src": "project_korea/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config":
            {
                "distDir": "staticfiles"
            }
        }
    ],
    "routes": 
    [
        {
            "src": "/(.*)",
            "dest": "project_korea/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}
